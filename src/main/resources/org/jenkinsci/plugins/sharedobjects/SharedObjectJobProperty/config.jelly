<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">

    <f:entry field="populateSharedObjects" title="${%Populate Shared Objects}">
        <f:checkbox
                name="populateSharedObjects"
                checked="${instance.populateSharedObjects}"/>
    </f:entry>

    <f:entry field="profiles" title="${%Restrict to selected profiles}">
        <f:textbox
                name="profiles"
                value="${instance.profiles}"/>
    </f:entry>

</j:jelly>
